#include <iostream>

int main(){
  std::cout << "Input width: ";
  std::cin >> int width;
  std::cout << "Input height: ";
  std::cin >> int height;
  int x = 0;
  std::string row;

  for(int i=0; i<width; i++){
    row += "*";
  }
  while (x < height){
    std::cout << row << std::endl;
  }
  
  return 0;
}
